  "jest": {
    "preset": "@testing-library/react-native",
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "png"
    ],
    "setupFiles": [
      "./jestSetup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/react-native/cleanup-after-each"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub"
